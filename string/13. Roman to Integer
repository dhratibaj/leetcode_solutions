class Solution:
    def romanToInt(self, s: str) -> int:
        c=0
        d={'I':1,'V':5,'X':10,'L':50,'C':100,'D':500,'M':1000}
        for i in range(len(s)):
            if s[i] in d:
                c += d[s[i]]
        for i in range(len(s)-1):
            if "IV" in s[i:i+2] or "IX" in s[i:i+2]:
                c -= 2
            elif "XL" in s[i:i+2] or "XC" in s[i:i+2]:
                c -= 20
            elif "CD" in s[i:i+2] or "CM" in s[i:i+2]:
                c -= 200
        return c
